function sonuc = AralarindaAsalMi(x, y)
    sonuc = 1; 
    for i = 2:x
        if (rem(x, i) == 0) && (rem(y, i) == 0)
            
            sonuc = 0;
            return;        
        end
    end
end

clc; clear;
x = input('1. Sayıyı Giriniz: ');
y = input('2. Sayıyı Giriniz: ');
cevap = AralarindaAsalMi(x, y);
fprintf(['Sonuç: ' num2str(cevap) '\n']);
