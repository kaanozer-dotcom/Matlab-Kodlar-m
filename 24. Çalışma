x = 0:0.1:10;
y1 = sin(x)
y2 = sin(2*x);
y3 = sin(4*x);
y4 = sin(8*x);

subplot(2,2,1)
plot(x,y1, '-r')
title('Subplot 1: sin(x)')

subplot(2,2,2)
plot(x,y2, '-b')
title('Subplot 2: sin(2x)')

subplot(2,2,3)
plot(x,y3, '-m')
title('Subplot 3: sin(4x)')

subplot(2,2,4)
plot(x,y4, '-c')
title('Subplot 4: sin(8x)')
